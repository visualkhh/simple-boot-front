{
  "name": "simple-boot-front-templates-parcel-bundler",
  "version": "1.0.0",
  "main": "index.js",
  "license": "MIT",
  "jest": {
    "transform": {
      "^.+\\.ts$": "ts-jest"
    },
    "testRegex": "\\.test\\.ts$",
    "moduleFileExtensions": [
      "ts",
      "tsx",
      "js",
      "json"
    ],
    "globals": {
      "ts-jest": {
        "diagnostics": true
      }
    }
  },
  "scripts": {
    "serve": "sbf serve --path ./dist --port 4500",
    "serve:watch": "sbf serve --path ./dist --port 4500 --watch",
    "serve:proxy8080": "sbf serve --path ./dist --port 4500 --proxy http://localhost:8080",
    "bundle": "rollup -c ",
    "bundle:watch": "rollup -c -w ",
    "start": "rollup -c && sbf serve --path ./dist --port 4500"
  },
  "devDependencies": {
    "@rollup/plugin-node-resolve": "^13.0.5",
    "@rollup/plugin-replace": "^2.4.2",
    "@types/jest": "^26.0.22",
    "@typescript-eslint/eslint-plugin": "^4.16.1",
    "@typescript-eslint/parser": "^4.16.1",
    "eslint": "^7.21.0",
    "eslint-config-standard": "^16.0.2",
    "eslint-plugin-import": "^2.22.1",
    "eslint-plugin-node": "^11.1.0",
    "eslint-plugin-promise": "^4.3.1",
    "jest": "^26.6.3",
    "rollup": "^2.58.0",
    "rollup-plugin-babel": "^4.4.0",
    "rollup-plugin-commonjs": "^10.1.0",
    "rollup-plugin-copy": "^3.4.0",
    "rollup-plugin-delete": "^2.0.0",
    "rollup-plugin-html": "^0.2.1",
    "rollup-plugin-import-css": "^3.0.2",
    "rollup-plugin-node-resolve": "^5.2.0",
    "rollup-plugin-sourcemaps": "^0.6.3",
    "rollup-plugin-typescript2": "^0.30.0",
    "rollup-watch": "^4.3.1",
    "simple-boot-front-cli": "^1.0.3",
    "supertest": "^6.1.3",
    "ts-jest": "^26.5.4",
    "typescript": "^4.4.3"
  },
  "dependencies": {
    "dom-render": "file:../libs/dom-render/dist",
    "simple-boot-front": "file:../dist"
  }
}
